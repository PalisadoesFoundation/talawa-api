{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"files": {
		"ignore": [
			"./node_modules",
			"./package.json",
			"./pnpm-lock.yaml",
			"./.docusaurus",
			"./build",
			"./.cache-loader",
			"./docs/auto-docs",
			"./static"
		],
		"ignoreUnknown": true
	},
	"linter": {
		"enabled": true
	},
	"organizeImports": {
		"enabled": true
	},
	"vcs": {
		"clientKind": "git",
		"defaultBranch": "develop",
		"enabled": true,
		"useIgnoreFile": true
	}
}
